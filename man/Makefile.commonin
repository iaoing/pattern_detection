.1in.1:
	@ echo Creating $@ man page...
	@ sed -e 's/#PACKAGE_STRING#/@PACKAGE_STRING@/g' \
	  -e 's/#AUTHORS#/$(AUTHORSREP)/g' \
	  -e 's/#SEEALSO#/$(SEEALSOREP)/g' \
	  -e 's/#COPYRIGHT#/$(COPYRIGHT)/g' \
	  > $@ < $<
	@ perl -pi -e 's/MYNL/\n/g' $@


